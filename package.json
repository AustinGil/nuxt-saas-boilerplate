{
  "name": "saas",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch api --exec \"nuxt\"",
    "knex": "node -r dotenv/config ./node_modules/knex/bin/cli.js --knexfile api/models/knexfile.js",
    "ts": "tsc",
    "audit": "npm run lint -- --fix && npm run ts && npm run test",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/composition-api": "^0.22.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@wesleytodd/openapi": "^0.1.0",
    "base-api-io": "^2.0.0",
    "core-js": "^3.8.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "gazepassjs": "^0.10.1",
    "helmet": "^4.4.1",
    "knex": "^0.95.2",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.4.17",
    "nuxt": "^2.14.12",
    "objection": "^2.2.14",
    "onegraph-auth": "^3.3.1",
    "pg": "^8.5.1",
    "pino-http": "^5.5.0",
    "postcss-for": "^2.1.1",
    "sparkpost": "^2.1.4",
    "ulid": "^2.3.0",
    "vuetensils": "^0.7.9",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@vue/test-utils": "^1.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.3.1",
    "eslint-plugin-jsdoc": "^32.2.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^23.0.0",
    "eslint-plugin-vue": "^7.5.0",
    "faker": "^5.1.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.7",
    "prettier": "^1.19.1",
    "sqlite3": "^5.0.2",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0",
    "supertest": "^6.0.1",
    "typescript": "^4.2.3",
    "vue-jest": "^3.0.4"
  }
}
